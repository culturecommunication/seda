<xsd:schema version="1.0" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
    xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" 
    xmlns="urn:un:unece:uncefact:codelist:draft:SIAF:unitIdentifier:2011-12-15" 
    xmlns:clmSIAFUnitIdentifierCode="urn:un:unece:uncefact:codelist:draft:SIAF:unitIdentifier:2011-12-15"
    targetNamespace="urn:un:unece:uncefact:codelist:draft:SIAF:unitIdentifier:2011-12-15"  
    elementFormDefault="qualified" attributeFormDefault="unqualified">
    
    
    <!-- ====================================================================== -->
    <!-- =====  Element Declarations				      ===== -->
    <!-- ====================================================================== -->
    <!-- =====  Root Element Declaration				      ===== -->
    <!-- ====================================================================== -->
    <xsd:element name="UnitIdentifierCode" type="clmSIAFUnitIdentifierCode:UnitIdentifierType"/>
    <!-- ====================================================================== -->
    <!-- ================ Type Definitions ============================= -->
    <!-- =============================================================== -->
    <!-- ===== Code List UnitIdentifierCode: Unit Identifier Code         ===== -->
    <!-- =============================================================== -->
    
    
    <xsd:simpleType name="UnitIdentifierType">
        <xsd:annotation>
            <xsd:documentation xml:lang="fr">
                <ccts:Name>Table des identifiants</ccts:Name>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="ArchivalAgencyArchiveIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant d'archive attribué par le service d'archive.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ArchivalAgencyObjectIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant d'objet d'archive attribué par le service d'archive.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="ArchivalAgencyDocumentIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant de document attribué par le service d'archive.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OriginatingAgencyArchiveIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant d'archive attribué par le service producteur.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OriginatingAgencyObjectIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant d'objet d'archive attribué par le service producteur.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OriginatingAgencyDocumentIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant de document attribué par le service producteur.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TransferringAgencyArchiveIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant d'archive attribué par le service versant.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TransferringAgencyObjectIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant d'objet d'archive attribué par le service versant.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="TransferringAgencyDocumentIdentifier">
                <xsd:annotation>
                    <xsd:documentation xml:lang="fr">
                        <ccts:Name>Identifiant de document attribué par le service versant.</ccts:Name>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>