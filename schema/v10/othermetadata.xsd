<xsd:schema version="1.0" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="fr:gouv:culture:archivesdefrance:seda:v1.0"
	targetNamespace="fr:gouv:culture:archivesdefrance:seda:v1.0" 
	elementFormDefault="qualified"
	attributeFormDefault="unqualified">

	<xsd:complexType name="OtherMetadataType">
		<xsd:annotation>
			<xsd:documentation xml:lang="fr">Les métadonnees doivent etre exprimées en XML et
				respecter le Schéma XML explicitement donné. </xsd:documentation>
		</xsd:annotation>
