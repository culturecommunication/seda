                                <ccts:Acronym>ABIE</ccts:Acronym>
                                <ccts:DictionaryEntryName>RelatedObjectReferenceType</ccts:DictionaryEntryName>
                                <ccts:Version>1.0</ccts:Version>
                                <ccts:Definition>Référence à un objet ne faisant pas partie du présent paquet d'information</ccts:Definition>
                                <ccts:ObjectClassTerm>RelatedObjectReferenceType</ccts:ObjectClassTerm>
                                <ccts:QualifierTerm>RelatedObjectReferenceType</ccts:QualifierTerm>
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="RelatedObjectIdentifier" type="qdt:ArchivesIDType">
