                                                <ccts:ObjectClassTerm>AccessRestrictionRule</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>StartDate</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                </xsd:sequence>
