<xsd:schema
        targetNamespace="fr:gouv:culture:archivesdefrance:seda:v1.0"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns="fr:gouv:culture:archivesdefrance:seda:v1.0"
        xmlns:udt="urn:un:unece:uncefact:data:standard:UnqualifiedDataType:10"
        xmlns:qdt="fr:gouv:culture:archivesdefrance:seda:v1.0:QualifiedDataType:1"
        xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2"
        elementFormDefault="qualified"
        attributeFormDefault="unqualified"
        version="1.0">


        <!-- ======================================================== -->
        <!-- ====                Includes                        ==== -->
        <!-- ======================================================== -->
        <xsd:include schemaLocation="seda_v1-0_organization.xsd"/>
        <xsd:include schemaLocation="othermetadata.xsd"/>

        <!-- ======================================================== -->
        <!-- ====                Imports                         ==== -->
        <!-- ======================================================== -->
        <!-- ======================================================== -->
        <!-- ====        Import of CoreComponentTypes            ==== -->
        <!-- ======================================================== -->
        <xsd:import namespace="urn:un:unece:uncefact:data:standard:UnqualifiedDataType:10" schemaLocation="UnqualifiedDataType_10p0.xsd"/>
        <xsd:import namespace="fr:gouv:culture:archivesdefrance:seda:v1.0:QualifiedDataType:1"  schemaLocation="seda_v1-0_QualifiedDataType.xsd"/>

        <!-- ======================================================== -->
        <!-- ====             Archive element                    ==== -->
        <!-- ======================================================== -->

        <xsd:element name="Archive" type="ArchiveType"/>

        <!-- ======================================================== -->
        <!-- ====                Type Definition                 ==== -->
        <!-- ======================================================== -->

        <xsd:complexType name="ArchiveType">
                <xsd:annotation>
                        <xsd:documentation>
                                <ccts:UniqueID>AAES000001</ccts:UniqueID>
                                <ccts:Acronym>ABIE</ccts:Acronym>
                                <ccts:DictionaryEntryName>Archive</ccts:DictionaryEntryName>
                                <ccts:Version>1.0</ccts:Version>
                                <ccts:Definition>Objet correspondant à un ensemble constitué d'objets-données, de leurs informations de représentation, 
                                        de leurs informations de pérennisation et de leurs informations de gestion. 
                                        Il s'agit du plus haut niveau de description de ce qui est échangé.</ccts:Definition>
                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                <ccts:QualifierTerm>Archive</ccts:QualifierTerm>
                        </xsd:documentation>
                </xsd:annotation>

                <xsd:sequence>

                        <xsd:element name="ArchivalAgencyArchiveIdentifier"  type="qdt:ArchivesIDType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-01</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.ArchivalAgencyArchiveIdentifier</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Identifiant attribué à l'&lt;Archive&gt; par le service d'archives.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>ArchivalAgencyArchiveIdentifier</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ArchivalAgreement" type="qdt:ArchivesIDType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-02</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.ArchivalAgreement</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Convention de services : porte sur les conditions qui pourront faire l'objet d'un contrôle
                                                        par le SAE pour une catégorie d'archives (périodicité de la transaction, volumétries, 
                                                        formats des données...).</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>ArchivalAgreement</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ArchivalProfile" type="qdt:ArchivesIDType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-03</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.ArchivalProfile</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Modèle de description/profil d'archivage pour une catégorie d'archives donnée.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>ArchivalProfile</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="DescriptionLanguage" type="qdt:CodeLanguageType" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-04</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.DescriptionLanguage</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>1..n</ccts:Cardinality>
                                                <ccts:Definition>Langue utilisée pour les informations de représentation et de pérennisation.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>DescriptionLanguage</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Name" type="udt:TextType">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-05</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.Name</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                                <ccts:Definition>Intitulé de l'&lt;Archive&gt;.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>Name</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OriginatingAgencyArchiveIdentifier"  type="qdt:ArchivesIDType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-06</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.OriginatingAgencyArchiveIdentifier</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Identifiant attribué à l'&lt;Archive&gt; par le service producteur.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>OriginatingAgencyArchiveIdentifier</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ServiceLevel" type="qdt:ArchivesCodeType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-07</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.ServiceLevel</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                                <ccts:Definition>Niveau de service demandé (disponibilité, sécurité...).</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>ServiceLevel</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="TransferringAgencyArchiveIdentifier"  type="qdt:ArchivesIDType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-08</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.TransferringAgencyArchiveIdentifier</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Identifiant attribué à l'&lt;Archive&gt; par le service versant.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>TransferringAgencyArchiveIdentifier</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ContentDescription" type="ContentDescriptionType">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-11</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.ContentDescription</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                                <ccts:Definition>Informations de pérennisation de l'&lt;Archive&gt;.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>ContentDescription</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="AccessRestrictionRule" type="AccessRestrictionRuleType">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-09</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.AccessRestrictionRule</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                                <ccts:Definition>Règle à appliquer en matière d'accès aux objets-données (communicabilité).</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>AccessRestrictionRule</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="AppraisalRule" type="AppraisalRuleType" minOccurs="0" maxOccurs="1">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-10</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.AppraisalRule</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Règle relative au cycle de vie du contenu d'information.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>AppraisalRule</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="ArchiveObject" type="ArchiveObjectType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-12</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.ArchiveObject</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                                <ccts:Definition>Objet correspondant à une subdivision de l'&lt;Archive&gt;.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>ArchiveObject</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Document" type="DocumentType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000001-13</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>Archive.Document</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                                <ccts:Definition>Objet correspondant à une subdivision de l'&lt;Archive&gt; ne pouvant pas lui-même 
                                                        être subdivisé et comportant obligatoirement un objet-données (fichier), ses informations de représentation
                                                        et ses informations de pérennisation.</ccts:Definition>
                                                <ccts:ObjectClassTerm>Archive</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>Document</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="Id" type="xsd:ID" use="optional"/>
        </xsd:complexType>

        <!-- ======================================================== -->
        <!-- ====        ContentDescription element              ==== -->
        <!-- ======================================================== -->

        <xsd:element name="ContentDescription" type="ContentDescriptionType"/>

        <!-- ======================================================== -->
        <!-- ====                Type Definition                 ==== -->
        <!-- ======================================================== -->

        <xsd:complexType name="ContentDescriptionType">
                <xsd:annotation>
                        <xsd:documentation>
                                <ccts:UniqueID>AAES000002</ccts:UniqueID>
                                <ccts:Acronym>ABIE</ccts:Acronym>
                                <ccts:DictionaryEntryName>ContentDescription</ccts:DictionaryEntryName>
                                <ccts:Version>1.0</ccts:Version>
                                <ccts:Definition>Informations de pérennisation d'une &lt;Archive&gt; ou d'un &lt;ArchiveObject&gt;.</ccts:Definition>
                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                <ccts:QualifierTerm>ContentDescription</ccts:QualifierTerm>
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>

                        <xsd:element name="Description" type="udt:TextType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-01</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.Description</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Permet de donner des précisions sur le contenu de l'objet.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>Description</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="DescriptionLevel" type="qdt:CodeDescriptionLevelType">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-02</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.DescriptionLevel</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                                <ccts:Definition>Niveau de description (Isad(g)) indiquant si l'objet 
                                                        &lt;Archive&gt; ou &lt;ArchiveObject&gt;. décrit est un groupe de documents, 
                                                        un sous-groupe de documents, un dossier, ou une pièce.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>DescriptionLevel</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>                        
                        
                        <xsd:element name="FilePlanPosition" type="qdt:ArchivesIDType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-03</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.FilePlanPosition</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                                <ccts:Definition>Position du contenu d'information dans le plan de classement du producteur.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>FilePlanPosition</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Language" type="qdt:CodeLanguageType" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-04</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.Language</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>1..n</ccts:Cardinality>
                                                <ccts:Definition>Langue du contenu des objets-données.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>Language</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="LatestDate" type="udt:DateType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-05</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.LatestDate</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Date de production la plus récente des objets-données.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>LatestDate</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OldestDate" type="udt:DateType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-06</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.OldestDate</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Date de production la plus ancienne des objets-données.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>OldestDate</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OtherDescriptiveData" type="udt:TextType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-07</ccts:UniqueID>
                                                <ccts:Acronym>BBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.OtherDescriptiveData</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Autres informations sur l'objet-données.</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>OtherDescriptiveData</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="AccessRestrictionRule" type="AccessRestrictionRuleType" minOccurs="0" maxOccurs="1">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-10</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.AccessRestrictionRule</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Règle à appliquer en matière d'accès aux informations de pérennisation (communicabilité)</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>AccessRestrictionRule</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="CustodialHistory" type="CustodialHistoryType" minOccurs="0">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-11</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.CustodialHistory</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                                <ccts:Definition>Énumère les changements successifs de propriété, de responsabilité et de conservation 
                                                        des objets-données avant leur entrée dans le lieu de conservation. On peut indiquer notamment 
                                                        comment s'est effectué le passage de l'application d'origine au fichier archivable..</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>CustodialHistory</ccts:PropertyTerm>
                                        </xsd:documentation>                                        
                                </xsd:annotation>
                        </xsd:element>
                        
                        <xsd:element name="Keyword" type="KeywordType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-08</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.Keyword</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
                                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                                <ccts:Definition>Mots-clef</ccts:Definition>
                                                <ccts:ObjectClassTerm>ContentDescription</ccts:ObjectClassTerm>
                                                <ccts:PropertyTerm>Keyword</ccts:PropertyTerm>
                                        </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OriginatingAgency" type="OrganizationType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                        <xsd:documentation>
                                                <ccts:UniqueID>AAES000002-12</ccts:UniqueID>
                                                <ccts:Acronym>ASBIE</ccts:Acronym>
                                                <ccts:DictionaryEntryName>ContentDescription.OriginatingAgency</ccts:DictionaryEntryName>
                                                <ccts:Version>1.0</ccts:Version>
