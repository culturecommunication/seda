							<ccts:Name>Binary Object. Algorithme. Text</ccts:Name>
							<ccts:Definition>Algorithme utilise pour encoder la valeur.</ccts:Definition>
							<ccts:PrimitiveType>string</ccts:PrimitiveType>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>			
		</xsd:simpleContent>
