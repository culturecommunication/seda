<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- ======================================================== -->
<!-- =====                                              ===== -->
<!-- =====                Archive                       ===== -->
<!-- =====                                              ===== -->
<!-- ======================================================== -->
<!--

  Last change: 20. February 2006

  Archive, standard d'echange 0.1
  Prefix: AAESx1

  Attention: cs schéma utilise le schéma archives_echanges_v0-1_organization.xsd
 
-->

<xsd:schema version="1.0" 
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns:ccts="urn:unece:uncefact:dataschema:draft:CoreComponentTypesSchemaModule:0:3:4" 
            xmlns="fr:gouv:ae:archive:draft:standard_echange_v0.1" 
            targetNamespace="fr:gouv:ae:archive:draft:standard_echange_v0.1"  
            elementFormDefault="qualified" attributeFormDefault="unqualified">


<!-- ======================================================== -->
<!-- ====                Imports                         ==== -->
<!-- ======================================================== -->
<!-- ======================================================== -->
<!-- ====        Import of CoreComponentTypes            ==== -->
<!-- ======================================================== -->

<xsd:import namespace="urn:unece:uncefact:dataschema:draft:CoreComponentTypesSchemaModule:0:3:4" schemaLocation="CoreComponentTypesSchemaModule_0.3.4.xsd"/>

<!-- ======================================================== -->
<!-- ====             Archive element                    ==== -->
<!-- ======================================================== -->

        <xsd:element name="Archive" type="ArchiveType"/>

<!-- ======================================================== -->
<!-- ====                Type Definition                 ==== -->
<!-- ======================================================== -->

        <xsd:complexType name="ArchiveType">
                <xsd:annotation>
                        <xsd:documentation>
                                <ccts:UniqueID>AAES000001</ccts:UniqueID>
                                <ccts:CategoryCode>ABIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Definition>Ensemble constitué d'un contenu d'information et de son information de pérennisation.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:QualifierTermName>Archive</ccts:QualifierTermName>
                        </xsd:documentation>
                </xsd:annotation>

                <xsd:sequence>

                        <xsd:element name="ArchivalAgencyArchiveIdentifier" type="ccts:IdentifierType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-01</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.ArchivalAgencyArchiveIdentifier</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Identifiant de l'archive attribué par le service d'archive. L'identifiant pour l'émetteur du message est recommandé, si ce n'est obligatoire.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>ArchivalAgencyArchiveIdentifier</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ArchivalAgreement" type="ccts:IdentifierType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-02</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.ArchivalAgreementr</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Indique la convention d'archivage à appliquer pour l'archive..</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>ArchivalAgreement</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ArchivalProfile" type="ccts:IdentifierType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-03</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.ArchivalProfile</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Indique la méthodologie de production de l'archive appliquée (structure adaptée au domaine particulier traité, par exemple archivage d'un dossier de marché public).</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>ArchivalProfile</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="DescriptionLanguage" type="ccts:CodeType">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-04</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.DescriptionLanguage</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                <ccts:Definition>Langue des descriptions.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>DescriptionLanguage</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="DescriptionLevel" type="ccts:CodeType">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-05</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.DescriptionLevel</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                <ccts:Definition>Indique si l'objet décrit est un groupe de documents, un sous-groupe de documents, un dossier, ou une pièce.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>DescriptionLevel</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Name" type="ccts:TextType">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-06</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.Name</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                <ccts:Definition>Intitulé du contenu d'information.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Name</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ServiceLevel" type="ccts:CodeType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-07</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.ServiceLevel</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Niveau de service demandé (disponibilité, sécurité...), en référence aux différents niveaux prévus par le contrat ou la convention passée entre le service versant et le service d'archives.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>ServiceLevel</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="TransferringAgencyArchiveIdentifier" type="ccts:IdentifierType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-08</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.TransferringAgencyArchiveIdentifier</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Identifiant d'archive fourni par le service versant. L'identifiant pour l'émetteur du message est recommandé, si ce n'est obligatoire..</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>TransferringAgencyArchiveIdentifier</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ContentDescription" type="ContentDescriptionType">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-09</ccts:UniqueID>
                                <ccts:CategoryCode>ASBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.ContentDescription</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                <ccts:Definition>Description du contenu de l'archive.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>ContentDescription</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Document" type="DocumentType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-10</ccts:UniqueID>
                                <ccts:CategoryCode>ASBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.Attached</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Documents attachés.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Attached</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Contains" type="ArchiveObjectType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000001-11</ccts:UniqueID>
                                <ccts:CategoryCode>ASBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>Archive.Contains</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Objet d'Archive.</ccts:Definition>
                                <ccts:ObjectClassTermName>Archive</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Contains</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                </xsd:sequence>
                <xsd:attribute name="Id" type="xsd:ID" use="optional"/>
        </xsd:complexType>

<!-- ======================================================== -->
<!-- ====        ContentDescription element              ==== -->
<!-- ======================================================== -->

        <xsd:element name="ContentDescription" type="ContentDescriptionType"/>  

<!-- ======================================================== -->
<!-- ====                Type Definition                 ==== -->
<!-- ======================================================== -->

        <xsd:complexType name="ContentDescriptionType">
                <xsd:annotation>
                        <xsd:documentation>
                                <ccts:UniqueID>AAES000002</ccts:UniqueID>
                                <ccts:CategoryCode>ABIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Definition>Informations sur le contenu de l'archive ou l'objet.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:QualifierTermName>ContentDescription</ccts:QualifierTermName>
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>

                        <xsd:element name="CustodialHistory" type="ccts:TextType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-01</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.CustodialHistory</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Enumère les changements successifs de propriété, de responsabilité et de conservation de l'objet avant son entrée dans le lieu de conservation. On peut indiquer notamment comment s'est effectué le passage de l'application d'origine au fichier archivable.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>CustodialHistory</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Description" type="ccts:TextType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-02</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.Description</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Permet de donner des précisions sur le contenu de l'objet. Il permet aussi de donner des précisions réservées aux professionnels et auxquelles le public ne doit pas avoir accès.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Description</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="DescriptionAudience" type="ccts:CodeType">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-03</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.DescriptionAudience</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>1..1</ccts:Cardinality>
                                <ccts:Definition>Permet d'indiquer si les précisions sur le contenu de l'objet sont à diffusion interne ou externe.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>DescriptionAudience</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="FilePlanPosition" type="ccts:IdentifierType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-04</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.FilePlanPosition</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Classement de l'objet transféré dans le (ou les) plan(s) de classement du (ou des) producteur.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>FilePlanPosition</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Format" type="ccts:TextType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-05</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.Format</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Permet d'indiquer d'autres formats respectés par l'objet que ceux qui sont mentionnés dans les éléments BinaryObject (par exemple: le fichier pdf contient du texte avec des balises XML).</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Format</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Language" type="ccts:CodeType" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-06</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.Language</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>1..n</ccts:Cardinality>
                                <ccts:Definition>Langue du contenu de l'objet.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Language</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="LatestDate" type="ccts:DateTimeType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-07</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.LatestDate</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Date de fin du contenu.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>LatestDate</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OldestDate" type="ccts:DateTimeType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-08</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.OldestDate</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Date et heure de début du contenu d'information.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>OldestDate</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OtherDescriptiveData" type="ccts:TextType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-09</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.OtherDescriptiveData</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition>Autres informations sur l'objet.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>OtherDescriptiveData</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="RelatedObjectReference" type="ccts:IdentifierType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-10</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.RelatedObjectReference</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Permet d'indiquer la référence d'un autre objet et la nature du lien avec ce dernier.</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>RelatedObjectReference</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Size" type="ccts:MeasureType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-11</ccts:UniqueID>
                                <ccts:CategoryCode>BBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.Size</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition>Taille de l'objet en octets, nombre d'entregistrements...</ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Size</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="OriginatingAgency" type="OrganizationType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-12</ccts:UniqueID>
                                <ccts:CategoryCode>ASBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.OriginatingAgency</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition></ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>OriginatingAgency</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Repository" type="OrganizationType" minOccurs="0">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-13</ccts:UniqueID>
                                <ccts:CategoryCode>ASBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.Repository</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..1</ccts:Cardinality>
                                <ccts:Definition></ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>Repository</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="ContentDescriptive" type="KeywordType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
                                <xsd:documentation>
                                <ccts:UniqueID>AAES000002-14</ccts:UniqueID>
                                <ccts:CategoryCode>ASBIE</ccts:CategoryCode>
                                <ccts:DictionaryEntryName>ContentDescription.ContentDescriptive</ccts:DictionaryEntryName>
                                <ccts:VersionID>000001</ccts:VersionID>
                                <ccts:Cardinality>0..n</ccts:Cardinality>
                                <ccts:Definition></ccts:Definition>
                                <ccts:ObjectClassTermName>ContentDescription</ccts:ObjectClassTermName>
                                <ccts:PropertyTermName>ContentDescriptive</ccts:PropertyTermName>
                                </xsd:documentation>
                                </xsd:annotation>
                        </xsd:element>

                        <xsd:element name="Appraisal" type="RulesType" minOccurs="0" maxOccurs="unbounded">
                                <xsd:annotation>
